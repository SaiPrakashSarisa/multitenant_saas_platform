			"item": [
				{
					"name": "Register (Get Token)",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"var jsonData = pm.response.json();",
									"if (jsonData.data && jsonData.data.token) {",
									"    pm.collectionVariables.set('token', jsonData.data.token);",
									"}"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"tenantName\": \"My E-commerce Store\",\n    \"slug\": \"my-ecommerce-store\",\n    \"businessType\": \"inventory\",\n    \"email\": \"owner@mystore.com\",\n    \"password\": \"Password123!\",\n    \"firstName\": \"Store\",\n    \"lastName\": \"Owner\"\n}"
						},
						"url": {
							"raw": "{{baseUrl}}/auth/register",
							"host": ["{{baseUrl}}"],
							"path": ["auth", "register"]
						}
					}
				},
